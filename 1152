import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		String word;
		word = sc.nextLine();
		
		String[] array = word.split(" ");
		int count =0;
		
		for(int i=0; i<array.length; i++) {
			if(array[i].equals("")) continue;
			else count++;
		}
		System.out.println(count);
		
	}
}
